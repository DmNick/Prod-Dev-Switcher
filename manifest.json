{
    "manifest_version": 3,
    "name": "Prod-Dev Switcher",
    "version": "1.0",
    "description": "Switcher z proda na deva i odwrotnie",
    "permissions": [
      "storage",
      "tabs"
    ],
    "options_page": "options.html",
    "background": {
      "service_worker": "background.js",
      "type": "module"
    },
    "action": {
      "default_icon": {
        "16": "ICON.png",
        "48": "ICON.png",
        "128": "ICON.png"
      }
    }
  }